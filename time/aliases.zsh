alias now-utc="python -c 'import datetime as dt; print(\"utc:\", dt.datetime.now(dt.UTC).strftime(\"%H:%M\"))'"
alias now-eastern="python -c 'import datetime as dt; import pytz as tz; print(\"eas:\", dt.datetime.now(tz.timezone(\"US/Eastern\")).strftime(\"%H:%M\"))'"
alias now-central="python -c 'import datetime as dt; import pytz as tz; print(\"central:\", dt.datetime.now(tz.timezone(\"US/Central\")).strftime(\"%H:%M\"))'"
alias now-pacific="python -c 'import datetime as dt; import pytz as tz; print(\"pacific:\", dt.datetime.now(tz.timezone(\"US/Pacific\")).strftime(\"%H:%M\"))'"
alias now-kiev="python -c 'import datetime as dt; import pytz as tz; print(\"Kiev:\", dt.datetime.now(tz.timezone(\"Europe/Kiev\")).strftime(\"%H:%M\"))'"
alias now-warsaw="python -c 'import datetime as dt; import pytz as tz; print(\"Warsaw:\", dt.datetime.now(tz.timezone(\"Europe/Warsaw\")).strftime(\"%H:%M\"))'"
alias now-tokyo="python -c 'import datetime as dt; import pytz as tz; print(\"Tokyo:\", dt.datetime.now(tz.timezone(\"Japan/Tokyo\")).strftime(\"%H:%M\"))'"
alias now-ist="python -c 'import datetime as dt; import pytz as tz; print(\"IST:\", dt.datetime.now(tz.timezone(\"Asia/Kolkata\")).strftime(\"%H:%M\"))'"
# alias now-central="python -c 'import datetime as dt; import pytz as tz; print(\"central:\", dt.datetime.now(tz.timezone(\"US/Eastern\")).strftime(\"%H:%M\"))'"
alias nows="now-utc; now-eastern; now-central; now-pacific"
